{
    "name": "movieenglish/amazon-sqs-job",
    "description": "This package is used to add all the required configuration and code to use and process your queues through amazon-sqs easily",
    "type": "library",
    "require": {
        "aws/aws-sdk-php": "3.1",
        "shiftonelabs/laravel-sqs-fifo-queue": "2.0",
        "php": ">=5.6.4",
        "laravel/framework": "^7.24"
    },
    "scripts": {
        "post-create-project-cmd": [],
        "post-install-cmd": [
            "php -r \"copy('app/Jobs/SampleJob.php', 'app/Jobs/SampleJob.php');\""
        ],
        "post-update-cmd": [
            "php -r \"copy('app/Jobs/SampleJob.php', 'app/Jobs/SampleJob.php');\""
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShiftOneLabs\\LaravelSqsFifoQueue\\LaravelSqsFifoQueueServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "MovieEnglish\\AmazonSqsJob\\": "src/app"
        }
    },
    "license": "MIT",
    "authors": [{
        "name": "Shuja Ahmed",
        "email": "shujahm@gmail.com"
    }],
    "minimum-stability": "dev"
}